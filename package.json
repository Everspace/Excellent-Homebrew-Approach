    "gatsby": "next",
    "gatsby-plugin-emotion": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-resolve-src": "next",
    "gatsby-remark-component": "Hebilicious/gatsby-remark-component#11/head",
