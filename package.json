    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.5",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.0.3"
  },
  "keywords": [
