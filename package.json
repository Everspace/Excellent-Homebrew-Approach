{
  "name": "exellent-homebrew-approach",
  "description": "A way to store homebrew for exalted, exellently",
  "version": "1.0.0",
  "author": "Aaron Haaf <aabonh@gmail.com>",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
