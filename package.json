{
  "name": "exellent-homebrew-approach",
  "description": "A way to store homebrew for exalted, exellently",
  "version": "1.0.0",
  "author": "Aaron Haaf <aabonh@gmail.com>",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "emotion": "^9.2.5",
    "emotion-server": "^9.2.5",
    "gatsby": "^2.0.55",
    "gatsby-plugin-emotion": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-resolve-src": "^1.1.4",
    "gatsby-plugin-typescript": "^2.0.1",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.12",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.5",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.1.0",
    "ts-node": "^7.0.1",
    "rollup": "^0.67.3",
    "rollup-plugin-typescript2": "^0.18.0",
    "typescript": "^3.1.1",
    "chalk": "^2.4.0",
    "dashify": "^2.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-plugin": "yarn rollup -c plugins/gatsby-remark-exalted/rollup.config.js",
    "build": "yarn build-plugin && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier-tslint fix 'src/**/*.js' 'plugins/gatsby-*/src/**/*'",
    "lint": "prettier-tslint check 'src/**/*' 'plugins/gatsby-*/src/**/*'",
    "test": "jest",
    "make-types": "env-cmd .env gql-gen --config ./gql-gen.json --schema http://localhost:8000/___graphql --template graphql-codegen-typescript-template --out ./types/GatsbyGraphQL.d.ts"
  },
  "resolutions": {
    "graphql": "0.13.2"
  },
  "devDependencies": {
    "@types/dashify": "^1.0.0",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.9",
    "@types/jest": "^23.3.9",
    "babel-jest": "^23.6.0",
    "env-cmd": "^8.0.2",
    "graphql-code-generator": "^0.12.6",
    "graphql-codegen-typescript-template": "^0.12.6",
    "jest": "^23.6.0",
    "prettier": "^1.12.0",
    "react-test-renderer": "^16.5.2",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Everspace/Exellent-Homebrew-Approach"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "/.*\\.test\\.ts?$",
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "src"
    ],
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "plugins",
      "dist"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  }
}
